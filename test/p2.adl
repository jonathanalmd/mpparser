
INIT(
    garbage () 
    AND
    clean()
    and
    quiet()
)

goal (
        at(c1,jfk) and at(c2,sfo)
)
action (
        load(c:cargo, c2:cargo, d:dargo),
        precond: at(c,a) and at(p,a)
        effect: at(c,a) and in(c,p)
)

action (
        unload(c:cargo,p:plane,a:air-port),
        precond: in(c,p) and at(p,a)
        effect: at(c,a) and in(c,p)
)